        <wizard category="aurora.ide.wizard" 
            class="aurora.ide.bm.wizard.sql.BMFromSQLWizard" 
            icon="icons/sql.gif" id="aurora.ide.wizard.BMFromSQLWizard" 
            name="%wizard.create.bm.from.sql">
        </wizard>
        <wizard category="aurora.ide.wizard" 
            class="aurora.ide.bm.wizard.procedure.BMFromProcedure" 
            icon="icons/sql.gif" id="aurora.ide.wizard.BMFromProcedure" 
            name="从数据库过程创建BM文件">
        </wizard>
