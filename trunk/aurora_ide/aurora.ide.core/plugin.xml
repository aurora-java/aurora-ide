<plugin>
    <extension point="org.eclipse.ui.newWizards">
        <category id="aurora.ide.wizard" name="%category.name">
        </category>
        <wizard category="aurora.ide.wizard"
            class="aurora.ide.project.wizard.NewAuroraProjectWizard" 
