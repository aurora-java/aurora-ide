<?xml version="1.0" encoding="UTF-8"?>
<template name="查询模板" iconPath="query.png">
	<models>
		<model id="model.id" name="model.name"/>
	</models>
	
	<result name="字段显示" id="result.id"  container="fieldset" model="model.id"/>
	
	<buttons name="buttons.name" id="buttons.id" container="hbox">
		<button type="save" text="保存" target="result.id"/>
		<button type="close" text="关闭"/>
	</buttons>

	<description><![CDATA[
		查询和修改数据
	]]></description>
</template>