<?xml version="1.0" encoding="UTF-8"?>
<template name="查询数据" icon="fg.png" type="serach" category="查询">
	<models>
		<model id="model_id" name="BM"/>
	</models>
	
	<form id="form_id" name="form_name"/>
	
	<hbox>
		<button id="btn_query" type="query" text="查询" target="grid_id"/>
		<button id="btn_reset" type="reset" text="重置" target="form_id"/>
	</hbox>
	
	<grid id="grid_id" name="grid_name" model="model_id" query="form_id">
		<toolBar>
			<button type="excel"/>
			<button type="clear"/>
		</toolBar>
	</grid>
	
	<description><![CDATA[
		通过Form查询Grid数据
	]]></description>
</template>