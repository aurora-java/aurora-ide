<?xml version="1.0" encoding="UTF-8"?>
<template name="查询数据" icon="t.png" type="serach" category="查询">
	<models>
		<model id="model_hid" name="头BM"/>
		<model id="model_rid" name="行BM"/>
	</models>
	
	<tabPanel>
		<tab>
			<grid id="grid_hid" name="grid_hname" model="model_hid">
				<toolBar>
					<button type="excel"/>
					<button type="clear"/>
				</toolBar>
			</grid>
		</tab>
		
		<tab>
			<grid id="grid_rid" name="grid_rname" model="model_rid" query="grid_hid">
				<toolBar>
					<button type="excel"/>
					<button type="clear"/>
				</toolBar>
			</grid>
		</tab>
	</tabPanel>
	<description><![CDATA[
		Tab放置头行结构的Grid
	]]></description>
</template>