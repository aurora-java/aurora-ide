<?xml version="1.0" encoding="UTF-8"?>
<template name="tabref引用示例" iconPath="1.png" type="serach" category="示例">
	<models>
		<model id="model_hid" name="头BM"/>
		<model id="model_rid" name="行BM"/>
		<initModel id="model_iid" name="initModel"/>
	</models>
	
	<form id="form_id" name="form_name"/>
	
	<hbox>
		<button id="btn_query" type="query" text="查询" target="grid_hid"/>
		<button id="btn_reset" type="reset" text="重置" target="form_id"/>
	</hbox>
	
	<tabFolder>
		<tabItem>
			<grid id="grid_hid" name="grid_hname" model="model_hid" query="form_id">
				<toolBar>
					<button type="add"/>
					<button type="save"/>
					<button type="delete"/>
					<button type="excel"/>
					<button type="clear"/>
				</toolBar>
			</grid>
		</tabItem>
		
		<tabItem>
			<grid id="grid_rid" name="grid_rname" model="model_rid" query="grid_hid">
				<toolBar>
					<button type="add"/>
					<button type="delete"/>
					<button type="excel"/>
					<button type="clear"/>
				</toolBar>
			</grid>
		</tabItem>
		
		<tabItem>
			<tabRef src="" initModel="model_iid"/>
		</tabItem>
	</tabFolder>
	
	<description><![CDATA[
		tabItem引用外部文件示例
	]]></description>
</template>