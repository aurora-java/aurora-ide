<?xml version="1.0" encoding="UTF-8"?>
<template name="headrow_serach" iconPath="1.png" type="serach" category="查询">
	<models>
		<model id="model.hid" name="model.hname"/>
		<model id="model.rid" name="model.rname"/>
	</models>
	
	<grid id="grid.hid" name="grid.hname" model="model.hid">
		<toolBar>
			<button type="add"/>
			<button type="save"/>
			<button type="delete"/>
			<button type="excel"/>
			<button type="clear"/>
		</toolBar>
	</grid>
	
	<grid id="grid.rid" name="grid.rname" model="model.rid" query="grid.hid">
		<toolBar>
			<button type="add"/>
			<button type="delete"/>
			<button type="excel"/>
			<button type="clear"/>
		</toolBar>
	</grid>
	
	<description><![CDATA[
		查询 form+grid+grid
	]]></description>
</template>