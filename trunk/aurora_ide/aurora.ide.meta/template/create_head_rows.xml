<?xml version="1.0" encoding="UTF-8"?>
<template name="general_create" iconPath="1.png" type="create" category="创建">
	<models>
		<model id="model_head" name="头表BM"/>
		<model id="model_detail" name="行表BM"/>
	</models>
	
	<form id="form.id" name="form.name" model="model_head" />
	
	<grid id="grid.id" name="grid.name" model="model_detail" query="form.id">
		<toolBar>
			<button type="add"/>
			<button type="save"/>
			<button type="delete"/>
		</toolBar>
	</grid>
	
	<description><![CDATA[
		以头行结构显示。创建头行结构的行数据，头表BM需要返回一条数据或者接收link到此页面的参数后返回一条数据。
	]]></description>
</template>