<?xml version="1.0" encoding="UTF-8"?>
<template name="tab" iconPath="1.png" type="serach" category="查询">
	<models>
		<model id="model.hid" name="model.hname"/>
		<model id="model.rid" name="model.rname"/>
		<initModel id="model.iid" name="model.iname"/>
	</models>
	
	<form id="form.id" name="form.name"/>
	
	<hbox>
		<button id="btn.query" type="query" text="查询" target="grid.hid"/>
		<button id="btn.reset" type="reset" text="重置" target="form.id"/>
	</hbox>
	
	<tabFolder>
		<tabItem>
			<grid id="grid.hid" name="grid.hname" model="model.hid" query="form.id">
				<toolBar>
					<button type="add"/>
					<button type="save"/>
					<button type="delete"/>
					<button type="excel"/>
					<button type="clear"/>
				</toolBar>
			</grid>
		</tabItem>
		
		<tabItem>
			<grid id="grid.rid" name="grid.rname" model="model.rid" query="grid.hid">
				<toolBar>
					<button type="add"/>
					<button type="save"/>
					<button type="delete"/>
					<button type="excel"/>
					<button type="clear"/>
				</toolBar>
			</grid>
		</tabItem>
		
		<tabItem>
			<tabRef src="" initModel="model.iid"/>
		</tabItem>
	</tabFolder>
	
	<description><![CDATA[
		查询 form+tabs
	]]></description>
</template>