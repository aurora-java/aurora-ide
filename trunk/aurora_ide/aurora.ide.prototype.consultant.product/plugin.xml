<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="aurora.ide.prototype.consultant.product.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Prototype Perspective"
            class="aurora.ide.prototype.consultant.product.Perspective"
            id="aurora.ide.prototype.consultant.product.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            name="File"
            id="aurora.ide.prototype.consultant.product.category">
      </category>
      <command
            name="Open File"
            description="Opens a new prototpye file"
            categoryId="aurora.ide.prototype.consultant.product.category"
            id="aurora.ide.prototype.consultant.product.open">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="aurora.ide.prototype.consultant.product.open"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+2">
      </key>
      <key
            commandId="org.eclipse.ui.file.exit"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+X">
      </key>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="aurora.ide.prototype.consultant.product.application"
            name="Aurora Quick UI Product">
         <property
               name="aboutText"
               value="Aurora Quick UI Product created by HAND MAS">
         </property>
         <property
               name="windowImages"
               value="icons/aurora.gif,product_lg.gif">
         </property>
         <property
               name="aboutImage"
               value="product_lg.gif">
         </property>
         <property
               name="appName"
               value="Aurora Quick UI Product">
         </property>
         <property
               name="startupProgressRect"
               value="5,275,445,15">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         
         <property
          		name="introTitle"
          		value="%productIntroTitle"/>
          <property
          		name="introBrandingImage"
          		value="product:intro-eclipse.png"/>
          <property
          		name="introBrandingImageText"
          		value="%productIntroBrandingText"/>
          <property
				name="introDescription-samples"
				value="%introDescription-samples"/>  
		<property
				name="samples-label"
				value="%samples-label"/>  
         
         
      </product>
   </extension>
   
   
 <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.ui.category.file"
            description="Open a file"
            id="aurora.ide.prototype.consultant.product.action.OpenFileAction"
            name="Open"/>
   </extension>
   
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="aurora.ide.prototype.consultant.product.action.OpenFileAction"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+O"/>
   </extension>
   




